package thread_test;

public class main {

	public static void main(String[] args) {
		obj_extends o1 = new obj_extends(1);
		o1.start();
		
		obj_extends o2 = new obj_extends(2);
		o2.start();
		
		thread_implements imp1 = new thread_implements();
		
		Thread thread=new Thread(this);//A클래스가 프레임이기 때문에 Thread가 일어날 대상을 인자로 넣어
		thread.start()//Thread를 참조하여 start()를 날려줘야 합니다.
		
	}

}
