		System.out.println(Integer.parseInt(십자가_현재.substring(0, 십자가_현재.length()-1)));
		System.out.println( 십자가_현재.charAt(십자가_현재.length()-1));
		
		int i_십자가_현재 = Integer.parseInt(십자가_현재.substring(0, 십자가_현재.length()-1))
				* (int)Math.pow(1000, (십자가_현재.charAt(십자가_현재.length()-1)-64));
