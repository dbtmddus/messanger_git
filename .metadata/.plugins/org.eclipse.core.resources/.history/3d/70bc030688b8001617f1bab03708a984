package wndsusrltk;

import java.math.BigInteger;
import java.util.Scanner;

public class main {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);

		System.out.println("십자가 ");
		System.out.println("현재 %: / 1업 폭 / 1업 비용");
		String 십자가_현재 =in.nextLine();
		//String 십자가_1업폭 =in.nextLine();
		//String 십자가_1업비용 =in.nextLine();
		
		System.out.println(Integer.parseInt(십자가_현재.substring(0, 십자가_현재.length()-1)));
		System.out.println( (십자가_현재.charAt(십자가_현재.length()-1)) -96);
		
		BigInteger bi_십자가_현재 = new BigInteger( Integer.toString(
				Integer.parseInt(십자가_현재.substring(0, 십자가_현재.length()-1))
				));
		for (int i=0; i< (십자가_현재.charAt(십자가_현재.length()-1)-96); i++){
			bi_십자가_현재 = bi_십자가_현재.multiply(BigInteger.valueOf(1000));
		}
		System.out.println(bi_십자가_현재);
		
		System.out.println();
		System.out.println();
		
		System.out.print("진힘반 - 현재 %: ");
		int 진힘반_현재 =in.nextInt();
		System.out.println("진힘반 - 1업 당 %: ");
		int 진힘반_1업당 =in.nextInt();
		System.out.println("진힘반 - 1업 비용 %: ");
		int 진힘반_1업비용 =in.nextInt();
		System.out.println();
		
		System.out.print("폭칼 - 현재 %: ");
		int 폭칼_현재 =in.nextInt();
		System.out.println("폭칼 - 1업 당 %: ");
		int 폭칼_1업당 =in.nextInt();
		System.out.println("폭칼 - 1업 비용 %: ");
		int 폭칼_1업비용 =in.nextInt();
		System.out.println();
		
		/*double 십자가효율 = (십자가_1업당/(100+십자가_현재))/십자가_1업비용;
		double 진힘반효율 = (십자가_1업당/(100+십자가_현재))/십자가_1업비용;
		double 폭칼효율 = (십자가_1업당/(100+십자가_현재))/십자가_1업비용;
		
		System.out.println("십자가 효율 : "+십자가효율 +
				"진힘반 효율: "+십자가효율 +
				"폭칼 효율 : "+십자가효율 );
				*/
	}//end main
}
