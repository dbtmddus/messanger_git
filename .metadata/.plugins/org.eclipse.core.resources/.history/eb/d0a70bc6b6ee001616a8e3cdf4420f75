package thread_sync;

public class thread_extends extends Thread{

	private int iden_n=0;
	private static int n=0;

	thread_extends(int _iden_n){
		iden_n= _iden_n;
	}	

	@Override
	public void run(){
		f();
	}

	public synchronized void f(){
		for(int i=0; i<100; i++){
			n=iden_n;
			System.out.println(iden_n +"'s n="+n);			
		}
	}
}