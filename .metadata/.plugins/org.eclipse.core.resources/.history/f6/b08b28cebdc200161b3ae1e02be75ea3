package messanger_test;

import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.io.IOException;

public class duplicate_file {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub

		FileInputStream in= new FileInputStream("a.jpg");
		FileOutputStream out = new FileOutputStream("a_duplicated5.jpg");
		
		int input_size;
		byte input_data;
		
		while ( (input_size = in.read() ) != -1) {
			input_data = (byte)input_size;
			out.write(input_data);
		}
		
		in.close();
		out.close();
	}//end main
}// end
