package test;
import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.JTextField;

public class swing_test extends JFrame{

	private JTextField ID_textfield;
	private JPanel panel;
	private JButton b1;
	
	public swing_test() {		
		super();

		getContentPane().setLayout(null);
		setVisible(true);
		setSize(260, 226);

		ID_textfield =new JTextField("aa");
		ID_textfield.setBounds(55, 143, 116, 24);	//안하면 안나옴 (아마 크기 0인듯)
		getContentPane().add(ID_textfield);						//안하면 안 나옴
		ID_textfield.addActionListener(action);	// 안 넣으면 이벤트 ㄴㄴ, 오류 주의
		
		panel = new JPanel();
		panel.setBounds(0, 0, 242, 32);	//안하면 안나옴 (아마 크기 0인듯)
		getContentPane().add(panel);						//안하면 안 나옴
		
		b1 =new JTextField("b1");
		b1.setBounds(55, 143, 116, 24);	//안하면 안나옴 (아마 크기 0인듯)
		getContentPane().add(b1);						//안하면 안 나옴
		b1.addActionListener(action);	// 안 넣으면 이벤트 ㄴㄴ, 오류 주의
	
	
	}//end creator

	ActionListener action = new ActionListener() {
		public void actionPerformed(ActionEvent e) {

			Object obj = e.getSource();
			/*if (obj==login_button){
			}
			else if (obj==signin_button){
				
			}*/
		}
	};
}//end

