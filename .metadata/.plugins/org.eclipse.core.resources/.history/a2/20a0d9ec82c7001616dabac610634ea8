public class obj {

	private int n1=0;
	private static int n2=0;
	
	public obj(){
		//n1=0;
	//	n2=0;
		f1();
		f2();
		//System.out.println(a);		
		
	}
	
	public void f1(){
		n1++;
		n2++;
		System.out.println(n1);
		System.out.println(n2);
	}
	public void f2(){
		System.out.println(n1);
		System.out.println(n2);
	}
}
