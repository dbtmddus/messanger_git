import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;
import java.io.IOException;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JScrollPane;
import javax.swing.JTextField;

public class scrollbar {

	public static void main(String[] args) {
		JFrame frame = new JFrame("");

		frame.getContentPane().setLayout(null);
		frame.setVisible(true);
		frame.setSize(260, 330);

		JScrollPane spanel = new JScrollPane();
		spanel.setBounds(58, 38, 137, 336);	//안하면 안나옴 (아마 크기 0인듯)
		frame.getContentPane().add(spanel);

		JTextField ID_textfield =new JTextField("aa");
		ID_textfield.setBounds(112, 128, 116, 24);	//안하면 안나옴 (아마 크기 0인듯)
		spanel.add(ID_textfield);						//안하면 안 나옴
	
		JTextField tf2 =new JTextField("aa");
		tf2.setBounds(120, 128, 166, 24);	//안하면 안나옴 (아마 크기 0인듯)
		spanel.add(tf2);						//안하면 안 나옴
	
	}//end creator


}//end

