package server_obj;

import java.sql.SQLException;

public class test_main {

	public static void main(String[] args) throws SQLException {
		DB_obj db = new DB_obj();
		db.connect();
		
		db.insert_new_client("client20000", 222);
		db.show_all();
		
		db.delete_all();
		db.show_all();
		
		db.disconnect();
	}
}
