package thread_test;

public int iden_n=0;
public int a=0;


public class thread_implements implements Runnable{
	public thread_implements(){
	}


	public void run(){
		while(true){
			if (a<100000){
				try {
					Thread.sleep(1000);
					a++;
					System.out.println(iden_n +" - "+a);
				} catch (InterruptedException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			}
		}
	}
	
}