package thread_sync;

public class thread_implements implements Runnable{
	public int iden_n=0;
	public int a=0;

	public thread_implements(int _iden_n){
		iden_n = _iden_n;
	}

	public void run(){
		while(true){
			if (a<100000){
				try {
					Thread.sleep(1000);
					a++;
					System.out.println(iden_n +" - "+a);
				} catch (InterruptedException e) {
					// TODO Auto-generated catch block
					e.printStackTrace();
				}
			}
		}
	}
}