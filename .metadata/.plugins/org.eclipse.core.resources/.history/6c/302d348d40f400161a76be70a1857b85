import java.util.Iterator;

public class test {

	public static void main(String[] args) throws UnknownHostException, IOException {
		String str ="";
	
		for (int i=0; i<10000; i++){
			str+=i;
			if (i%10==0){
				str+="\n"
			}
		}
		System.out.println(str);
	}//end main
	
}
